<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="accountA" class="com.myang03.Banking.Account" scope="prototype">
		<constructor-arg index="0" value="1001" type="int" />
		<constructor-arg index="1" value="360.23" type="double" />
	</bean>

	<bean id="userA" class="com.myang03.Banking.User" scope="prototype">
		<constructor-arg index="0" value="userA" type="java.lang.String" />
	</bean>
</beans>
